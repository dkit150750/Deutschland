{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["prettier", "unicorn", "import"],
	"extends": ["eslint:recommended", "airbnb-base", "prettier", "plugin:import/recommended", "plugin:unicorn/recommended", "plugin:prettier/recommended"],

	"rules": {
		"prettier/prettier": "error",
		// unicorn
		"unicorn/no-array-for-each": "off",
		"unicorn/no-null": "off",
		"unicorn/prefer-module": "off",
		"unicorn/prefer-node-protocol": "off",
		"unicorn/filename-case": "off"
	}
}
